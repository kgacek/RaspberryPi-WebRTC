# Docker Compose Environment Variables
# Copy this file to .env and fill in your values

# Cloudflare Calls Credentials
CF_REALTIME_APP_ID=your-cloudflare-app-id-here
CF_REALTIME_TOKEN=your-cloudflare-token-here

# ArcadeRally Backend
ARCADERALLY_API=http://localhost:3000/api
CAR_ID=your-car-id-here
CAR_API_KEY=car_your_api_key_here

# Optional: Logging
LOG_LEVEL=info

# Optional: Camera settings (override in docker-compose.yml if needed)
# CAMERA_FPS=25
# CAMERA_WIDTH=1280
# CAMERA_HEIGHT=720

# Optional: UART settings
# UART_DEVICE=/dev/ttyS0
# UART_BAUD=115200
